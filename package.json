{
  "name": "sfdx-autoheader",
  "displayName": "SFDX Autoheader",
  "description": "Automatic Template-Based File Header Generation for SFDX",
  "version": "0.2.2",
  "publisher": "HugoOM",
  "license": "MIT",
  "badges": [{
    "url": "https://travis-ci.org/HugoOM/sfdx_autoheader.svg?branch=master",
    "href": "https://travis-ci.org/HugoOM/sfdx_autoheader",
    "description": "Travis-CI Build Status for Master Branch (Current Release)"
  }],
  "preview": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/HugoOM/sfdx_autoheader"
  },
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "apex",
    "visualforce",
    "Salesforce",
    "File Header",
    "Comments"
  ],
  "icon": "images/slds_action_follow_icon.png",
  "galleryBanner": {
    "theme": "dark",
    "color": "#232323"
  },
  "activationEvents": [
    "onLanguage:apex",
    "onLanguage:visualforce"
  ],
  "main": "./extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "SFDX Autoheader Config",
      "properties": {
        "SFDX_Autoheader.username": {
          "type": "string",
          "default": "phUser@phDomain.com",
          "description": "Sets the SFDX File Header Username"
        }
      }
    },
    "languages": [{
      "id": "apex",
      "filenames": [
        "testFile_SFDXAutoheader.apex"
      ]
    }, {
      "id": "visualforce",
      "filenames": [
        "testFile_SFDXAutoheader.page"
      ]
    }]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.25",
    "chai": "^4.2.0",
    "eslint": "^4.11.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21"
  }
}